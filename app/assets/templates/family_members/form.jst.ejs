<form class='family-member-form'>

  <label for="fname">First Name</label>
  <input
    id="name"
    type="text"
    name="family_member[fname]"
    value="<%= familyMember && familyMember.escape('fname') %>">

  <label for="lname">Last Name</label>
  <input
    id="name"
    type="text"
    name="family_member[lname]"
    value="<%= familyMember && familyMember.escape('lname') %>">

  <button><%= familyMember.isNew() ? "Add to Family" : "Submit Edit" %></button>
</form>
